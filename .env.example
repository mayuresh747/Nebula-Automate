# nebulaONE API Configuration
# Rename this file to .env and fill in your values

# Your nebulaONE JWT token (required)
# Automatically fetched from browser or Get it from browser console: JSON.parse(localStorage.getItem('persist:auth')).n1aiToken
NEBULA_AUTH_TOKEN=your_jwt_token_here

# Nebula Config State ID (Specifies the model/configuration to use)
# Example: 65cf4c89-cfd5-48d1-a6d7-0e78ba568266
NEBULA_CONFIG_STATE_ID=your_config_state_id_here

# GPT System ID (optional)
NEBULA_GPT_SYSTEM_ID=your_gpt_system_id_here

# Base URL 
NEBULA_BASE_URL=https://nebulaone-pilot.uw.edu

# API Key for securing your wrapper API (optional but recommended)
# If set, clients must include this in X-API-Key header
# API_KEY=your_secret_api_key_here
